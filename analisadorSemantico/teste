/*Programa sugerido pelo grupo*/
program
	integer : number1, number2, count;
	//comentarios com barras duplas não são aceitos na linguagem orion;
	//comentarios em Orion devem estar entre /**/
begin
	/*palavras reservadas*/
	while, repeat, if, else, return, exit, read, write, integer, char, boolean, entre outros;
	number1 := 1;
	number2 := 7;
	count := 1;
	while count <= number2)
		number1 := number1 * count;
		count++;
	endwhile
	return 0;
end
